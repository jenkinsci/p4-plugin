<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
 	
	<f:entry>
		<f:checkbox title="${%REPLACE missing/modified files}" field="replace" default="true"/>
	</f:entry>
	
	<f:entry>
		<f:checkbox title="${%DELETE generated files}" field="delete" default="true"/>
	</f:entry>
	
	<f:entry field="pin">
		<table border="0" width="100%">
			<tr>
				<td nowrap="true">Pin build at Perforce Label</td>
				<td width="100%"><f:textbox/></td>
			</tr>
		</table>
	</f:entry>
		  
</j:jelly>